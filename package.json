{
    "name": "address-book-v2",
    "version": "1.0.0",
    "description": "Address manager",
    "main": "app.ts",
    "scripts": {
        "dev": "docker-compose up --build --force-recreate --remove-orphans",
        "test": "docker-compose exec address-book npm run test:local",
        "test:local": "jest --coverage --detectOpenHandles",
        "lint": "docker-compose exec address-book npm run lint:local:json",
        "lint:local": "eslint app.ts src test",
        "lint:local:json": "eslint app.ts src test -f json -o eslint_report.json",
        "start": "ts-node-dev app.ts",
        "start:prod": "ts-node --transpileOnly app.ts",
        "migrate": "docker-compose exec address-book npm run migrate:local -- ",
        "migrate:local": "ts-node-dev migration-scripts/migrate/index.ts",
        "rollback": "docker-compose exec address-book npm run rollback:local -- ",
        "rollback:local": "ts-node-dev migration-scripts/rollback/index.ts",
        "check:solr": "migration-scripts/check-solr-config.sh"
    },
    "keywords": [],
    "author": "Redspher",
    "private": "true",
    "dependencies": {
        "@elastic/ecs-pino-format": "^1.3.0",
        "@elastic/elasticsearch": "^8.8.1",
        "@fastify/formbody": "^7.4.0",
        "awilix": "^8.0.1",
        "axios": "^1.4.0",
        "elastic-apm-node": "^3.48.0",
        "fastify": "^4.19.2",
        "is-uuid": "^1.0.2",
        "isomorphic-fetch": "^3.0.0",
        "moment-timezone": "^0.5.43",
        "nock": "^13.3.2",
        "pino": "^8.14.1",
        "pino-elasticsearch": "^6.5.0",
        "pino-std-serializers": "^6.2.2",
        "rtech-struct": "^2.38.1",
        "superstruct": "^1.0.3",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/elasticsearch": "^5.0.40",
        "@types/jest": "^29.5.3",
        "@types/node": "^18.17.1",
        "@types/node-fetch": "^2.6.4",
        "@types/uuid": "^9.0.2",
        "@typescript-eslint/eslint-plugin": "^6.1.0",
        "@typescript-eslint/parser": "^6.1.0",
        "eslint": "^8.45.0",
        "eslint-config-airbnb-typescript": "^17.1.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jest": "^27.2.3",
        "jest": "^29.6.1",
        "ts-jest": "^29.1.1",
        "ts-node-dev": "^2.0.0"
    }
}
